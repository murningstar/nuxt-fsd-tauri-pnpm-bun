{
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
    "productName": "nuxt-fsd-tauri-pnpm-bun",
    "version": "0.1.0",
    "identifier": "com.tauri.dev",
    "build": {
        "frontendDist": "../dist",
        "devUrl": "http://localhost:1337",
        "beforeDevCommand": "pnpm dev",
        "beforeBuildCommand": "bun install --lockfile-only && bun generate"
    },
    "app": {
        "windows": [
            {
                "title": "nuxt-fsd-tauri-pnpm-bun",
                "width": 800,
                "height": 600,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
    }
}
